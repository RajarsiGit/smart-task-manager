{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/auth/github/callback",
      "destination": "/api/auth/github/callback"
    },
    {
      "source": "/api/auth/github",
      "destination": "/api/auth/github"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api/auth?path=login"
    },
    {
      "source": "/api/auth/logout",
      "destination": "/api/auth?path=logout"
    },
    {
      "source": "/api/auth/me",
      "destination": "/api/auth?path=me"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "env": {}
}
